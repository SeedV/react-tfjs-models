{
  "env": {
    "browser": true,
    "node" : true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "linebreak-style": 0
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "ignorePatterns": [
    "dist/*",
    "node_modules/*"
  ]
}
